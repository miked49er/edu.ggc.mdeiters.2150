public void stutter(Stack<Integer> ints) {

    Queue<Integer> que = new LinkedList<Integer>();
    
    while (!ints.isEmpty()) {
        que.add(ints.pop());
    }
    while (!que.isEmpty()) {
        ints.push(que.remove());
    }
